@article{brown_correlations_1980,
	title = {Correlations for diffusion constants},
	volume = {28},
	issn = {0001-6160},
	url = {http://www.sciencedirect.com/science/article/pii/0001616080900929},
	doi = {10.1016/0001-6160(80)90092-9},
	abstract = {Correlations for the melting-point diffusivity (DTM) the activation energy (Q) and the activation volume (V*) for diffusion are re-examined, using the best available diffusion data for metals and ceramics. The data support the postulates that, for a given crystal structure and bond type DTM = AQRT0M = B and V* = Q0T0M(dTMdp)0 where A and B are constants which, to first order, are independent of pressure, p; T0M is the meltingpoint at atmospheric pressure; and R is the gas constant. Mean values of the constants A and B, and values for V*, are evaluated for twelve crystal structures and bond types. For each such class the diffusion behaviour can be written D = Aexp - B exp - \{B {\textperiodcentered} T0MT (1 = p dTM0T0M dp)\} Such equations are useful in estimating approximate rates of transport, or creep, or sintering, in materials for which little or no diffusion data are available.},
	number = {8},
	urldate = {2015-03-03},
	journal = {Acta Metallurgica},
	author = {Brown, A. M. and Ashby, M. F.},
	month = aug,
	year = {1980},
	pages = {1085--1101},
	file = {Brown and Ashby - 1980 - Correlations for diffusion constants.pdf:/home/ken/.zotero/zotero/profile.default/zotero/storage/W4GUKNGI/Brown and Ashby - 1980 - Correlations for diffusion constants.pdf:application/pdf}
}

