@article{simmons_measurements_1960,
	title = {Measurements of {Equilibrium} {Vacancy} {Concentrations} in {Aluminum}},
	volume = {117},
	url = {http://link.aps.org/doi/10.1103/PhysRev.117.52},
	doi = {10.1103/PhysRev.117.52},
	abstract = {Measurements of change in length and change in lattice parameter were made at identical temperatures on 99.995\% aluminum in the temperature range 229 to 656{\textdegree}C. Length changes, $\Delta$L, were measured on an unconstrained horizontal bar sample using a rigid pair of filar micrometer microscopes. X-ray lattice parameter changes, $\Delta$a, were observed using a high-angle, back-reflection, rotating-single-crystal technique. The measurements are compared to earlier work. The relative expansions $\Delta$LL and $\Delta$aa were equal within about 1:105 from 229 to 415{\textdegree}C. At higher temperatures additional atomic sites were found to be generated: the difference between the two expansions could be represented by 3($\Delta$LL-$\Delta$aa)=exp(2.4)exp(-0.76 ev/kT). At the melting point (660{\textdegree}C) the equilibrium concentration of additional sites is 3($\Delta$LL-$\Delta$aa)=9.4{\texttimes}10-4. This result is independent of the detailed nature of the defects, for example, the lattice relaxation or degree of association. The nature of the defects is considered and it is concluded that they are predominantly lattice vacancies; it is estimated that the divacancy contribution at the melting point may well be less than about 15\%, corresponding to a divacancy binding energy ? 0.25 ev. The observed formation energy agrees with the values obtained by quenching techniques and by interpretation of the high-temperature electrical resistivity of identical material by Simmons and Balluffi. The present work is the first direct measurement of formation entropy; the value is near that expected from theoretical considerations. The contribution of the thermally generated defects to other physical properties at high temperatures is considered briefly.},
	number = {1},
	urldate = {2015-03-11},
	journal = {Physical Review},
	author = {Simmons, R. O. and Balluffi, R. W.},
	month = jan,
	year = {1960},
	pages = {52--61},
	file = {Simmons and Balluffi - 1960 - Measurements of Equilibrium Vacancy Concentrations.pdf:/home/ken/.zotero/zotero/profile.default/zotero/storage/TIB7JFNU/Simmons and Balluffi - 1960 - Measurements of Equilibrium Vacancy Concentrations.pdf:application/pdf}
}

