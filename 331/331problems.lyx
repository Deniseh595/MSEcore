\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\eta_{0}=\intop_{0}^{\infty}G(t)=\intop_{0}^{\infty}\left[G_{1}\exp\left(\frac{-t}{\tau_{1}}\right)+G_{2}\exp\left(\frac{-t}{\tau_{2}}\right)\right]dt
