	author = {Kapadia, Muneera R. and Chow, Lesley W. and Tsihlis, Nick D. and Ahanchi, Sadaf S. and Eng, Jason W. and Murar, Jozef and Martinez, Janet and Popowich, Daniel A. and Jiang, Qun and Hrabie, Joseph A. and Saavedra, Joseph E. and Keefer, Larry K. and Hulvat, James F. and Stupp, Samuel I. and Kibbe, Melina R.},
	month = jan,
