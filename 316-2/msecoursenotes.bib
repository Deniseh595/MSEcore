@article{seitz_fracture_2009,
	title = {Fracture and Large Strain Behavior of Self-Assembled Triblock Copolymer Gels},
	volume = {5},
	copyright = {100 web},
	url = {http://dx.doi.org/10.1039/b810041a},
	doi = {10.1039/b810041a},
	abstract = {The rate dependence of fracture has been studied in a series of physically associating triblock copolymer gels that have a well-defined molecular structure. Compressive experiments were performed to develop a strain energy function that accurately captures the strain hardening 10 behavior of these materials. This same strain energy function was utilized in a finite element model of the crack tip stresses, which become highly anisotropic at stress values below the failure strength of the gels. The rate dependence is independent of the gel concentration when the energy release rate, G, is normalized by the small strain Young's modulus, E. The gels exhibit a transition from rough, slow crack propagation to smooth, fast crack propagation for a well-defined value of 15 the characteristic length, G/E.},
	journal = {Soft Matter},
	author = {Seitz, Michelle E. and Martina, David and Baumberger, Tristan and Krishnan, Venkat R. and Hui, Chung-Yuen and Shull, Kenneth R.},
