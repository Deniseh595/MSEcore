@article{sacca_dna_2012,
	title = {{DNA} Origami: The Art of Folding {DNA}},
	volume = {51},
	copyright = {Copyright {\textcopyright} 2012 {WILEY}-{VCH} Verlag {GmbH} \& Co. {KGaA}, Weinheim},
	issn = {1521-3773},
	shorttitle = {{DNA} Origami},
	url = {http://onlinelibrary.wiley.com/doi/10.1002/anie.201105846/abstract},
	doi = {10.1002/anie.201105846},
	abstract = {The advent of {DNA} origami technology greatly simplified the design and construction of nanometer-sized {DNA} objects. The self-assembly of a {DNA}-origami structure is a straightforward process in which a long single-stranded scaffold (often from the phage M13mp18) is folded into basically any desired shape with the help of a multitude of short helper strands. This approach enables the ready generation of objects with an addressable surface area of a few thousand nm2 and with a single {\textquotedblleft}pixel{\textquotedblright} resolution of about 6 nm. The process is rapid, puts low demands on experimental conditions, and delivers target products in high yields. These features make {DNA} origami the method of choice in structural {DNA} nanotechnology when two- and three-dimensional objects are desired. This Minireview summarizes recent advances in the design of {DNA} origami nanostructures, which open the door to numerous exciting applications.},
	language = {en},
	number = {1},
	urldate = {2013-03-06},
	journal = {Angewandte Chemie International Edition},
	author = {Sacc{\`a}, Barbara and Niemeyer, Christof M.},
	year = {2012},
	keywords = {design, {DNA} origami, {DNA} sequence design, nanostructures, self-assembly, supramolecuar chemistry},
	pages = {58--66},
	file = {Sacc{\`a} and Niemeyer - 2012 - DNA Origami The Art of Folding DNA.pdf:/home/ken/.zotero/zotero/profile.default/zotero/storage/7RJJ4QX7/Sacc{\`a} and Niemeyer - 2012 - DNA Origami The Art of Folding DNA.pdf:application/pdf}
}

