	doi = {10.1016/j.biomaterials.2005.04.018},
	abstract = {In order to improve the bonding between hydroxyapatite ({HAP}) particles and poly(l-lactide) ({PLLA}), and hence to increase mechanical properties of the {PLLA}/{HAP} composite as potential bone substitute material, the {HAP} nano-particles were surface-grafted with {PLLA} and further blended with {PLLA}. The structure and properties of the composites were subsequently investigated by the mechanical property testing, the differential scanning calorimeter measurements ({DSC}), the scanning electron microscopy ({SEM}), the polarized optical microscopy ({POM}), and the cell culture. The {PLLA} molecules grafted on the {HAP} surfaces, as inter-tying molecules, played an important role in improving the adhesive strength between the particles and the polymer matrix. At a low content (\~{}4 wt\%) of surface grafted-{HAP} (g-{HAP}), the {PLLA}/g-{HAP} nano-composites exhibited higher bending strength and impact energy than the pristine {PLLA}, and at a higher g-{HAP} content (e.g., 20 wt\%), the modulus was remarkably increased. It implied that {PLLA} could be strengthened as well as toughened by g-{HAP} nano-particles. The results of biocompatibility test showed that the g-{HAP} existing in the {PLLA} composite facilitated both adhesion and proliferation of chondrocytes on the {PLLA}/g-{HAP} composite film.},
	number = {32},
	urldate = {2013-11-04},
	journal = {Biomaterials},
	author = {Hong, Zhongkui and Zhang, Peibiao and He, Chaoliang and Qiu, Xueyu and Liu, Aixue and Chen, Li and Chen, Xuesi and Jing, Xiabin},
	month = nov,
	year = {2005},
	pages = {6296--6304},
	file = {Hong et al. - 2005 - Nano-composite of poly(l-lactide) and surface graf.pdf:/home/ken/.zotero/zotero/profile.default/zotero/storage/ZXJQFG4U/Hong et al. - 2005 - Nano-composite of poly(l-lactide) and surface graf.pdf:application/pdf}
}

@article{rogers_materials_2010,
