@article{montgomery_thermoreversible_2002,
	title = {Thermoreversible Gelcasting: A Novel Ceramic Processing Technique},
	volume = {85},
	copyright = {056 web},
	abstract = {A new ceramic processing method, thermoreversible gelcasting ({TRG}) has been developed. This method uses a gelation process that can be reversed multiple times. Two alumina systems have been explored with the use of a specific triblock copolymer to form a binding gel network. The mixture becomes a free flowing liquid upon heating above 60 C. However , upon cooling below 60 C, the slurry instantaneously tranforms to a physical gel. This process can be reversed easily (essentially with an infinite processing time window) while the solvent is present in the system, which is advantageious when trying to produce high quality dense pieces if initial casting irregularities occur. Near theoretically dense specifments have been produced with properties consistent with reported values of high-density alumina.},
	number = {5},
	journal = {J. Amer. Ceramic Soc.},
	author = {Montgomery, John K. and Drzal, Peter L. and Shull, Kenneth R. and Faber, K.T.},
	year = {2002},
	pages = {1164--1168},
	file = {Montgomery et al. - 2002 - Thermoreversible Gelcasting A Novel Ceramic Proce.pdf:/home/ken/.zotero/zotero/profile.default/zotero/storage/D8C38IDT/Montgomery et al. - 2002 - Thermoreversible Gelcasting A Novel Ceramic Proce.pdf:application/pdf}
