@article{wu_generalized_1988,
	title = {A generalized criterion for rubber toughening: The critical matrix ligament thickness},
	volume = {35},
	copyright = {Copyright {\textcopyright} 1988 John Wiley \& Sons, Inc.},
	issn = {1097-4628},
	shorttitle = {A generalized criterion for rubber toughening},
	url = {http://onlinelibrary.wiley.com/doi/10.1002/app.1988.070350220/abstract},
	doi = {10.1002/app.1988.070350220},
	abstract = {The thickness of matrix ligament is shown to be the single parameter determining whether a polymer/rubber blend will be tough or brittle. The matrix ligament is defined as the region of the matrix between two neighboring rubber particles. Specifically, the ligament thickness is the surface-to-surface interparticle distance. When the average ligament thickness is smaller than a critical value, a blend will be tough; when greater, it will be brittle. In other words, a sharp brittle{\textendash}tough transition occurs at the critical ligament thickness. This critical parameter is independent of rubber volume fraction and particle size, and is characteristic of the matrix for a given mode, temperature and rate of deformation. What is important is the matrix ligament, not rubber particles. The single matrix parameter explains the effects of phase morphology, rubber volume fraction, particle size, particle-size polydispersity, and particle flocculation on toughness.},
	language = {en},
	number = {2},
	urldate = {2013-03-08},
	journal = {Journal of Applied Polymer Science},
	author = {Wu, Souheng},
	year = {1988},
	pages = {549--561},
	file = {Full Text PDF:/home/ken/.zotero/zotero/profile.default/zotero/storage/6VDBQDMD/Wu - 1988 - A generalized criterion for rubber toughening The.pdf:application/pdf;Snapshot:/home/ken/.zotero/zotero/profile.default/zotero/storage/CHFXFPFE/abstract.html:text/html}
}

@article{wu_impact_1983,
	title = {Impact fracture mechanisms in polymer blends: Rubber-toughened nylon},
	volume = {21},
	copyright = {Copyright {\textcopyright} 1983 John Wiley \& Sons, Inc.},
	issn = {1542-9385},
	shorttitle = {Impact fracture mechanisms in polymer blends},
	url = {http://onlinelibrary.wiley.com/doi/10.1002/pol.1983.180210503/abstract},
	doi = {10.1002/pol.1983.180210503},
	abstract = {The impact fracture mechanisms of rubber-toughened nylon are analyzed by measuring the energy dissipated in several different processes during notched fracture. An energy balance is thus established for the impact fracture. The stress-whitened zone is shown to be the energy dissipation zone, having an energy dissipation density of 5.30 cal/g. About 25\% (i.e., 1.44 cal/g) of the impact energy is dissipated by matrix crazing. About 75\% (i.e., 3.86 cal/g) is dissipated as heat by matrix yielding, causing a temperature rise of 9.1 {\textdegree}C in the energy dissipation zone. Surface energy storage on the fracture surfaces is negligible (i.e., 1.45 {\texttimes} 10-5 cal/g). During matrix yielding, about 12\% of $\alpha$-triclinic nylon crystals is transformed into pseudohexagonal crystals.},
	language = {en},
	number = {5},
	urldate = {2013-03-08},
	journal = {Journal of Polymer Science: Polymer Physics Edition},
	author = {Wu, Souheng},
	year = {1983},
	pages = {699--716},
	file = {Full Text PDF:/home/ken/.zotero/zotero/profile.default/zotero/storage/EDEKVB4I/Wu - 1983 - Impact fracture mechanisms in polymer blends Rubb.pdf:application/pdf;Snapshot:/home/ken/.zotero/zotero/profile.default/zotero/storage/RIXBJUIX/abstract.html:text/html}
}

@article{wong_effect_2000,
	title = {Effect of rubber functionality on microstructures and fracture toughness of impact-modified nylon 6,6/polypropylene {blendsPart} {II}. Toughening mechanisms},
	volume = {41},
	issn = {0032-3861},
	url = {http://www.sciencedirect.com/science/article/pii/S0032386199007508},
	doi = {10.1016/S0032-3861(99)00750-8},
	abstract = {Toughening mechanisms in blends containing 60 parts nylon 6,6, 20 parts polypropylene ({PP}) and 20 parts styrene{\textendash}ethylene/butylene{\textendash}styrene ({SEBS}) grafted to different levels of maleic anhydride ({MA}) were investigated. The sequence of events was carefully characterised using different microscopic techniques. It was found that under triaxial constraint interfacial cavitation followed by multiple crazing and subsequently massive shear yielding of the matrix contributed to an enormous toughening effect in core-shell microstructures observed in 0.92\%-maleated blend (0.74\% in Part I paper [Wong {SC}, Mai Y-W. Polymer, 1999;40:1553], should be 0.92\% as corrected in this paper). The core-shell structure was formed when the spherical domains of {PP} were surrounded by {SEBS} rubber in a nylon-rich matrix.. In this composition, miscibility between the dispersed {SEBS}-g-{MA} and the nylon phase was maximised as revealed by thermal-mechanical analysis. The {SEBS} was most effective in toughening the nylon/{PP} blends when it cavitated to introduce ligament bridges between debonded {PP} particles at the crack tip. Interfacial cavitation and multiple crazing served to relieve the hydrostatic tension ahead of crack growth and subsequently enhanced the shear-yielding component of stresses in the matrix material. Other blend compositions that did not show controlled cavitation resulted in little plastic flow surrounding the crack tip and reduced fracture toughness. These results reinforced the notion that cavitation of {SEBS} at the nylon{\textendash}{PP} interface was an essential mechanism to promote toughening in materials subjected to high crack tip triaxiality.},
	number = {14},
	urldate = {2013-03-08},
	journal = {Polymer},
	author = {Wong, Shing-Chung and Mai, Yiu-Wing},
	month = jun,
	year = {2000},
	keywords = {blends, Nylon 6,6, {SEBS}},
	pages = {5471--5483},
	file = {ScienceDirect Full Text PDF:/home/ken/.zotero/zotero/profile.default/zotero/storage/J8IEUPE4/Wong and Mai - 2000 - Effect of rubber functionality on microstructures .pdf:application/pdf;ScienceDirect Snapshot:/home/ken/.zotero/zotero/profile.default/zotero/storage/STJ3FK2P/S0032386199007508.html:text/html}
}

