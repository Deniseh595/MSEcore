	pages = {355--369},
	file = {Brown et al. - 2006 - Biological effects of clinically relevant wear par.pdf:/home/ken/.zotero/zotero/profile.default/zotero/storage/RF59A662/Brown et al. - 2006 - Biological effects of clinically relevant wear par.pdf:application/pdf}
}

