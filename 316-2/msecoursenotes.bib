	file = {Tibbitt et al. - 2013 - Mechanical Properties and Degradation of Chain and.pdf:/home/ken/.zotero/zotero/profile.default/zotero/storage/N869F2WF/Tibbitt et al. - 2013 - Mechanical Properties and Degradation of Chain and.pdf:application/pdf}
}

@article{sakai_gelation_2013,
