@article{atkin_direct_2000,
	title = {Direct visualization of changes in deacylated Na+ gellan polymer morphology during the sol{\textendash}gel transition},
	volume = {54},
	copyright = {Copyright {\textcopyright} 2000 John Wiley \& Sons, Inc.},
	issn = {1097-0282},
	url = {http://onlinelibrary.wiley.com.turing.library.northwestern.edu/doi/10.1002/1097-0282(200009)54:3<195::AID-BIP60>3.0.CO;2-#/abstract},
	doi = {10.1002/1097-0282(200009)54:3<195::AID-BIP60>3.0.CO;2-#},
	abstract = {Changes in gellan polymer morphology during the sol{\textendash}gel transition were directly visualized by transmission electron microscopy and a model incorporating these changes and existing physical data is proposed. Our observations suggest that the most thermodynamically stable conformations of gellan polymers in solution, in the absence of added cations, are the double helix and double-helical duplexes. We have demonstrated two forms of lateral aggregation of gellan helices in the presence of Ca2+ and K+ ions. One type forms junction zones that lead to network formation and gelation, while the second type leads to the formation of isolated fibers of aggregated helices and inhibition of gelation. The proposed model of gellan gelation is based on these observations where thermoreversibility, gel strength, and endothermic transitions of gellan gels can be explained. {\textcopyright} 2000 John Wiley \& Sons, Inc. Biopoly 54: 195{\textendash}210, 2000},
	language = {en},
	number = {3},
	urldate = {2013-03-13},
	journal = {Biopolymers},
	author = {Atkin, Neil and Abeysekera, Rukmal M. and Kronestedt-Robards, Eva C. and Robards, Anthony W.},
	year = {2000},
	keywords = {deacylated Na+ gellan, junction zones, sol{\textendash}gel transition, transmission electron microscopy},
	pages = {195--210},
	file = {Snapshot:/home/ken/.zotero/zotero/profile.default/zotero/storage/TDTTVU2Z/abstract\;jsessionid=59E741960207E87BCB216AB799620471.html:text/html}
}

@article{arkus_minimal_2009,
	title = {Minimal Energy Clusters of Hard Spheres with Short Range Attractions},
	volume = {103},
	url = {http://link.aps.org/doi/10.1103/PhysRevLett.103.118303},
	doi = {10.1103/PhysRevLett.103.118303},
	abstract = {We calculate the ground states of hard-sphere clusters, in which n identical hard spherical particles bind by isotropic short-ranged attraction. Combining graph theoretic enumeration with basic geometry, we analytically solve for clusters of n<=10 particles satisfying minimal rigidity constraints. For n<=9 the ground state degeneracy increases exponentially with n, but for n{\textgreater}9 the degeneracy decreases due to the formation of structures with {\textgreater}3n-6 contacts. Interestingly, for n=10 and possibly at n=11 and n=12, the ground states of this system are subsets of hexagonal close-packed crystals. The ground states are not icosahedra at n=12 and n=13. We relate our results to the structure and thermodynamics of suspensions of colloidal particles with short-ranged attractions.},
	number = {11},
	urldate = {2013-03-13},
	journal = {Physical Review Letters},
	author = {Arkus, Natalie and Manoharan, Vinothan N. and Brenner, Michael P.},
