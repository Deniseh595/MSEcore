@article{erk_strain_2010,
	title = {Strain Stiffening in Synthetic and Biopolymer Networks},
	volume = {11},
	copyright = {114 web},
	issn = {{PMID}: 20392048},
	url = {http://dx.doi.org.turing.library.northwestern.edu/10.1021/bm100136y},
	doi = {10.1021/bm100136y},
	abstract = {Strain-stiffening behavior common to biopolymer networks is difficult to reproduce in synthetic networks. Physically associating synthetic polymer networks can be an exception to this rule and can demonstrate strain-stiffening behavior at relatively low values of strain. Here, the stiffening behavior of model elastic networks of physically associating triblock copolymers is characterized by shear rheometry. Experiments demonstrate a clear correlation between network structure and strain-stiffening behavior. Stiffening is accurately captured by a constitutive model with a single fitting parameter related to the midblock length. The same model is also effective for describing the stiffening of actin, collagen, and other biopolymer networks. Our synthetic polymer networks could be useful model systems for biological materials due to (1) the observed similarity in strain-stiffening behavior, which can be quantified and related to network structure, and (2) the tunable structure of the physically associating network, which can be manipulated to yield a desired response.},
	urldate = {2010-04-14},
	journal = {Biomacromolecules},
	author = {Erk, Kendra A. and Henderson, Kevin J. and Shull, Kenneth R.},
	year = {2010},
	pmid = {20392048},
	keywords = {Biopolymers, Biopolymers: chemistry},
	pages = {1358--1363},
	file = {Erk et al. - 2010 - Strain Stiffening in Synthetic and Biopolymer Netw.pdf:/home/ken/.zotero/zotero/profile.default/zotero/storage/72PFW2EF/Erk et al. - 2010 - Strain Stiffening in Synthetic and Biopolymer Netw.pdf:application/pdf}
}

@article{carvajal_physical_2010,
	title = {Physical properties of hierarchically ordered self-assembled planar and spherical membranes},
	volume = {6},
