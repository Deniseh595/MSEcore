@article{eriksson_mechanically_1999,
	title = {On the mechanically unstable free energy minimum of a gas bubble which is submerged in water and adheres to a hydrophobic wall},
	volume = {159},
	issn = {0927-7757},
	url = {http://www.sciencedirect.com.turing.library.northwestern.edu/science/article/B6TFR-3XX6F1B-H/2/b861d591e24f8d96826a6e6f8db5ed63},
	doi = {10.1016/S0927-7757(99)00171-5},
	abstract = {Free energy minimization may seem like an universally valid method to single out the thermodynamic equilibrium states among a certain set of imaginable system states. Nevertheless, especially for capillary systems, it happens that false minima arise in the free energy functions chosen which are incompatible with the Young contact angle condition. In the present paper we elucidate this question for the case of a gas bubble submerged in water and adhering to a planar hydrophobic wall at constant chemical potentials. In particular, it is shown that by assuming a priori the Laplace condition, one finds a mechanically unstable (i.e. false) minimum of geometric origin when the contact angle [theta] is equal to 90{\textdegree}, and, in addition, a (correct) maximum at some angle [theta]{\textgreater}90{\textdegree} where the Young condition is fulfilled. On the other hand, still assuming a spherical bubble shape but considering the [Omega]-potential of the contact angle system to be a function of two geometric variables (bubble radius, r and height, h, instead of just h), only one extremum is obtained: The maximum arising for an angle [theta]{\textgreater}90{\textdegree}. Physically, our analysis implies that stable attachment of a small gas bubble to an ideal, molecularly smooth, hydrophobic solid surface at constant chemical potentials will not occur.},
	number = {1},
	urldate = {2010-09-24},
	journal = {Colloids and Surfaces A: Physicochemical and Engineering Aspects},
	author = {Eriksson, Jan Christer and Ljunggren, Stig},
	month = nov,
	year = {1999},
	pages = {159--163},
	file = {Eriksson and Ljunggren - 1999 - On the mechanically unstable free energy minimum o.pdf:/home/ken/.zotero/zotero/profile.default/zotero/storage/25RDQVMG/Eriksson and Ljunggren - 1999 - On the mechanically unstable free energy minimum o.pdf:application/pdf}
}

