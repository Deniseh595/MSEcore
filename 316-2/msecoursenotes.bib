	file = {ScienceDirect Full Text PDF:/home/ken/.zotero/zotero/profile.default/zotero/storage/S94S3WCE/Kumar et al. - 2009 - Nanoscale particles for polymer degradation and st.pdf:application/pdf;ScienceDirect Snapshot:/home/ken/.zotero/zotero/profile.default/zotero/storage/4U8UIN2M/S0079670009000112.html:text/html}
}

