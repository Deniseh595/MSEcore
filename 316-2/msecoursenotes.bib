@article{mahomed_swelling_2010,
	title = {Swelling of medical grade silicones in liquids and calculation of their cross-link densities},
	volume = {32},
	issn = {1350-4533},
	url = {http://www.sciencedirect.com/science/article/pii/S1350453309002653},
	doi = {10.1016/j.medengphy.2009.12.004},
	abstract = {Four medical grade silicones were swollen, until they reached equilibrium (i.e. constant mass) in eight liquids at 25 {\textdegree}C. The greatest swelling was obtained with n-heptane but the volume fraction, $\phi$, of the silicones in their swollen state was not significantly different (p {\textless} 0.05) in this liquid than in cyclohexane. For each grade of silicone, $\phi$ was plotted against $\delta$l, the liquid solubility parameter, for each liquid in which it was swollen. A second-order polynomial was plotted through the results; the minimum in this polynomial provided a value for the polymer solubility parameter, $\delta$p. The Flory polymer{\textendash}liquid interaction parameter, $\chi$, was calculated for the four best liquids, using Hildebrand's solubility parameter theory. An alternative method for calculating $\chi$, directly from swelling measurements, was shown to produce physically unreasonable results. The cross-link density, $\upsilon$, was calculated, from $\phi$ and $\chi$, for each grade of silicone, using the Flory{\textendash}Rehner equation. Since the values of two parameters involved in Hildebrand's theory cannot be determined reliably and because the Flory{\textendash}Rehner equation is an approximation, absolute values of $\upsilon$ cannot be obtained. However, the relative values of $\upsilon$ obtained were higher for the harder grades then for the softer grades and similarly, the grades with the higher Young's modulus had higher $\upsilon$ values.},
	number = {4},
	urldate = {2014-03-14},
	journal = {Medical Engineering \& Physics},
	author = {Mahomed, Aziza and Hukins, David W. L. and Kukureka, Stephen N.},
	month = may,
	year = {2010},
	keywords = {Cross-link density, Flory{\textendash}Rehner equation, Hildebrand's solubility parameter theory, Silicones, Swelling of polymers in liquids},
	pages = {298--303},
	file = {Mahomed et al. - 2010 - Swelling of medical grade silicones in liquids and.pdf:/home/ken/.zotero/zotero/profile.default/zotero/storage/27KPCACD/Mahomed et al. - 2010 - Swelling of medical grade silicones in liquids and.pdf:application/pdf}
}

@article{higgins_simple_2010,
	title = {A simple approach to polymer mixture miscibility},
	volume = {368},
	issn = {1364-503X, 1471-2962},
	url = {http://rsta.royalsocietypublishing.org/content/368/1914/1009},
	doi = {10.1098/rsta.2009.0215},
	abstract = {Polymeric mixtures are important materials, but the control and understanding of mixing behaviour poses problems. The original Flory{\textendash}Huggins theoretical approach, using a lattice model to compute the statistical thermodynamics, provides the basic understanding of the thermodynamic processes involved but is deficient in describing most real systems, and has little or no predictive capability. We have developed an approach using a lattice integral equation theory, and in this paper we demonstrate that this not only describes well the literature data on polymer mixtures but allows new insights into the behaviour of polymers and their mixtures. The characteristic parameters obtained by fitting the data have been successfully shown to be transferable from one dataset to another, to be able to correctly predict behaviour outside the experimental range of the original data and to allow meaningful comparisons to be made between different polymer mixtures.},
	language = {en},
	number = {1914},
