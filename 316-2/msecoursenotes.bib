@article{maune_self-assembly_2010,
	title = {Self-assembly of carbon nanotubes into two-dimensional geometries using {DNA} origami templates},
	volume = {5},
	copyright = {{\textcopyright} 2009 Nature Publishing Group},
	issn = {1748-3387},
	url = {http://www.nature.com.turing.library.northwestern.edu/nnano/journal/v5/n1/full/nnano.2009.311.html},
	doi = {10.1038/nnano.2009.311},
	abstract = {A central challenge in nanotechnology is the parallel fabrication of complex geometries for nanodevices. Here we report a general method for arranging single-walled carbon nanotubes in two dimensions using {DNA} origami{\textemdash}a technique in which a long single strand of {DNA} is folded into a predetermined shape. We synthesize rectangular origami templates ({\textasciitilde}75 nm {\texttimes} 95 nm) that display two lines of single-stranded {DNA} {\textquoteleft}hooks{\textquoteright} in a cross pattern with {\textasciitilde}6 nm resolution. The perpendicular lines of hooks serve as sequence-specific binding sites for two types of nanotubes, each functionalized non-covalently with a distinct {DNA} linker molecule. The hook-binding domain of each linker is protected to ensure efficient hybridization. When origami templates and {DNA}-functionalized nanotubes are mixed, strand displacement-mediated deprotection and binding aligns the nanotubes into cross-junctions. Of several cross-junctions synthesized by this method, one demonstrated stable field-effect transistor-like behaviour. In such organizations of electronic components, {DNA} origami serves as a programmable nanobreadboard; thus, {DNA} origami may allow the rapid prototyping of complex nanotube-based structures.},
	number = {1},
	urldate = {2013-03-14},
	journal = {Nature Nanotechnology},
	author = {Maune, Hareem T. and Han, Si-ping and Barish, Robert D. and Bockrath, Marc and Iii, William A. Goddard and Rothemund, Paul W. K. and Winfree, Erik},
	year = {2010},
	keywords = {astronanotechnology, atomic nanotechnology, bionanotechnology, chemical nanotechnology, condensed matter, content, cosmology, electronics, fluid dynamics, geonanotechnology, high energy nanotechnology, journal, molecular nanotechnology, Nanotechnology, nature, nature nanotechnology, nature publishing group, nonlinear dynamics, nuclear nanotechnology, optical nanotechnology, particle nanotechnology, photonics, physical scientist, physicist, plasma nanotechnology, quantum nanotechnology, statistical nanotechnology, thermodynamics},
	pages = {61--66},
	file = {Full Text PDF:/home/ken/.zotero/zotero/profile.default/zotero/storage/T732775M/Maune et al. - 2010 - Self-assembly of carbon nanotubes into two-dimensi.pdf:application/pdf}
