	volume = {27},
	issn = {0885-3282, 1530-8022},
	url = {http://jba.sagepub.com/content/27/2/219},
	doi = {10.1177/0885328211399480},
	abstract = {An off-the-shelf vascular graft biomaterial for vascular bypass surgeries is an unmet clinical need. The vascular biomaterial must support cell growth, be non-thrombogenic, minimize intimal hyperplasia, match the structural properties of native vessels, and allow for regeneration of arterial tissue. Electrospun recombinant human tropoelastin ({rTE}) as a medial component of a vascular graft scaffold was investigated in this study by evaluating its structural properties, as well as its ability to support primary smooth muscle cell adhesion and growth. {rTE} solutions of 9, 15, and 20 wt\% were electrospun into sheets with average fiber diameters of 167 {\textpm} 32, 522 {\textpm} 67, and 735 {\textpm} 270 nm, and average pore sizes of 0.4 {\textpm} 0.1, 5.8 {\textpm} 4.3, and 4.9 {\textpm} 2.4 {\textmu}m, respectively. Electrospun {rTE} fibers were cross-linked with disuccinimidyl suberate to produce an insoluble fibrous polymeric recombinant tropoelastin ({prTE}) biomaterial. Smooth muscle cells attached via integrin binding to the {rTE} coatings and proliferated on {prTE} biomaterials at a comparable rate to growth on {prTE} coated glass, glass alone, and tissue culture plastic. Electrospun tropoelastin demonstrated the cell compatibility and design flexibility required of a graft biomaterial for vascular applications.},
	language = {en},
	number = {2},
	urldate = {2013-03-12},
	journal = {Journal of Biomaterials Applications},
	author = {McKenna, Kathryn A. and Gregory, Kenton W. and Sarao, Rebecca C. and Maslen, Cheryl L. and Glanville, Robert W. and Hinds, Monica T.},
	month = aug,
	year = {2012},
	keywords = {cell adhesion, cell proliferation, elastin, Tissue engineering, vascular biomaterial},
	pages = {219--230},
	file = {Full Text PDF:/home/ken/.zotero/zotero/profile.default/zotero/storage/I6IKSNSR/McKenna et al. - 2012 - Structural and cellular characterization of electr.pdf:application/pdf;Snapshot:/home/ken/.zotero/zotero/profile.default/zotero/storage/TJD7JHRD/219.html:text/html}
}

