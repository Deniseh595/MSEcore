	file = {PLoS Full Text PDF:/home/ken/.zotero/zotero/profile.default/zotero/storage/8DRDTFMG/Chiu et al. - 2013 - Evaluation of Physical and Mechanical Properties o.pdf:application/pdf}
}

@article{wang_injectable_2010,
	title = {Injectable biodegradable hydrogels with tunable mechanical properties for the stimulation of neurogenesic differentiation of human mesenchymal stem cells in 3D culture},
