@article{kreuer_investigation_1981,
	title = {Investigation of proton-conducting solids},
	volume = {3{\textendash}4},
	issn = {0167-2738},
	url = {http://www.sciencedirect.com/science/article/pii/0167273881901120},
	doi = {10.1016/0167-2738(81)90112-0},
	abstract = {One-dimensional bulk proton conduction parallel to the c axis was observed in solid lithium hydrazinium sulfate, {LiN}2H5SO4. The conductivity in this direction is 2 {\texttimes} 10-8 $\omega$-1 cm-1 at 25{\textdegree}C and shows an activation enthalpy of 0.75 {\textpm} 0.07 {eV}. The two-dimensional conductors {HUO}2AsO4{\textperiodcentered}4H2O and {HUO}2PO4{\textperiodcentered}4H2O were studied as a function of their water content. The conductivities are 8 {\texttimes} 10-6 and 3 {\texttimes} 10-5 $\omega$-1 cm-1 in the orthorhombic phase at -10{\textdegree}C, with activation enthalpies of 0.70 {\textpm} 0.05 and 0.57 {\textpm} 0.07 {eV}, respectively. Indications of peritectic transitions to the tetragonal phases were observed in the temperature ranges 15 to 47 and -5 to 10{\textdegree}C, respectively. The transition depends on the water content which appears to control the increase in conductivity in this material. The dependence upon various sample parameters is discussed. Fast proton transport in solids is proposed to occur by a {\textquotedblleft}vehicle mechanism{\textquotedblright}, i.e. the motion of N2H+5, H3O+- or other proton-containing groups.},
	urldate = {2014-02-09},
	journal = {Solid State Ionics},
	author = {Kreuer, K. D. and Weppner, W. and Rabenau, A.},
	month = aug,
	year = {1981},
	pages = {353--358}
}

