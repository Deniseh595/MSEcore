	url = {http://onlinelibrary.wiley.com/doi/10.1002/jbm.a.34434/abstract},
	doi = {10.1002/jbm.a.34434},
	abstract = {For blood vessel tissue engineering, an ideal vascular graft should possess excellent biocompatibility and mechanical properties. For this study, a elastic material of poly (L-lactic acid-co-$\epsilon$-caprolactone) (P({LLA}-{CL})), collagen and chitosan blended scaffold at different ratios were fabricated by electrospinning. Upon fabrication, the scaffolds were evaluated to determine the tensile strength, burst pressure, and dynamic compliance. In addition, the contact angle and endothelial cell proliferation on the scaffolds were evaluated to demonstrate the structures potential to serve as a vascular prosthetic capable of in situ regeneration. The collagen/chitosan/P({LLA}-{CL}) scaffold with the ratio of 20:5:75 reached the highest tensile strength with the value of 16.9 {MPa}, and it was elastic with strain at break values of \~{}112\%, elastic modulus of 10.3 {MPa}. The burst pressure strength of the scaffold was greater than 3365 {mmHg} and compliance value was 0.7\%/100 {mmHg}. Endothelial cells proliferation was significantly increased on the blended scaffolds versus the P({LLA}-{CL}). Meanwhile, the endothelial cells were more adherent based on the increase in the degree of cell spreading on the surface of collagen/chitosan/P({LLA}-{CL}) scaffolds. Such blended scaffold especially with the ratio of 20:5:75 thus has the potential for vascular graft applications. {\textcopyright} 2012 Wiley Periodicals, Inc. J Biomed Mater Res Part A, 2012.},
	language = {en},
	urldate = {2013-03-09},
	journal = {Journal of Biomedical Materials Research Part A},
	author = {Yin, Anlin and Zhang, Kuihua and McClure, Michael J. and Huang, Chen and Wu, Jinglei and Fang, Jun and Mo, Xiumei and Bowlin, Gary L. and Al-Deyab, Salem S. and El-Newehy, Mohamed},
	year = {2012},
	keywords = {Biocompatibility, collagen/chitosan/P({LLA}-{CL}) blend, electrospinning, mechanical properties, vascular graft},
	pages = {n/a--n/a},
	file = {Full Text PDF:/home/ken/.zotero/zotero/profile.default/zotero/storage/247SWEID/Yin et al. - 2012 - Electrospinning collagenchitosanpoly(L-lactic ac.pdf:application/pdf;Snapshot:/home/ken/.zotero/zotero/profile.default/zotero/storage/FM9VD7ZK/full.html:text/html}
}

