@article{gibson_dynamic_1978,
	title = {Dynamic mechanical behaviour and longitudinal crystal thickness measurements on ultra-high modulus linear polyethylene: a quantitative model for the elastic modulus},
	volume = {19},
	issn = {0032-3861},
