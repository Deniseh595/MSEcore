@article{anders_computational_2012,
	title = {Computational modeling of phase separation and coarsening in solder alloys},
	volume = {49},
	issn = {0020-7683},
	url = {http://www.sciencedirect.com/science/article/pii/S0020768312001114},
	doi = {10.1016/j.ijsolstr.2012.03.018},
	abstract = {Solders represent highly versatile and useful materials. They provide a broad range of technical applications such as soldering in automotive processing, microelectromechanical systems ({MEMS}) and solar panels. Due to the fascinating variety of microstructural changes solder materials underlie, their micromorphological dynamics have been extensively studied in the past decades by experimental, analytical and numerical approaches. The evolved microstructure exerts a significant effect, in particular, in very small components such as solder joints in microelectronic packages. In order to capture the essence of the microstructural evolution in solder alloys with a diffusion theory of heterogeneous solid mixtures we employ an extended Cahn{\textendash}Hilliard phase-field model. In our contribution we introduce different numerical schemes to treat Cahn{\textendash}Hilliard equation. Here we focus on the innovative isogeometric finite element approach and outline its considerable benefits in comparison to the other methods. To this end we present numerical simulations of phase decomposition and coarsening controlled by diffusion for eutectic binary solders Sn{\textendash}Pb and Ag{\textendash}Cu illustrating the versatility of this approach. A concluding computational study of a three-dimensional phase separation event within a solder ball geometry will corroborate the quality of our model.},
	number = {13},
	urldate = {2012-11-09},
	journal = {International Journal of Solids and Structures},
	author = {Anders, Denis and Hesch, Christian and Weinberg, Kerstin},
	month = jun,
	year = {2012},
	pages = {1557--1572},
	file = {Anders et al. - 2012 - Computational modeling of phase separation and coa.pdf:/home/ken/.zotero/zotero/profile.default/zotero/storage/TDSIQJS2/Anders et al. - 2012 - Computational modeling of phase separation and coa.pdf:application/pdf}
}

@article{hawbolt_kinetics_1985,
	title = {Kinetics of austenite-ferrite and austenite-pearlite transformations in a 1025 carbon steel},
	volume = {16},
	issn = {1073-5623},
	url = {http://www.springerlink.com/content/f6l81550503551t7/abstract/},
	doi = {10.1007/BF02814230},
	abstract = {Isothermal and continuous-cooling transformation kinetics have been measured dilatometrically for the $\gamma$ {\textrightarrow} $\alpha$ + $\gamma$' and $\gamma$' {\textrightarrow} P reactions in a 1025 steel. The isothermal transformation of austenite for each reaction was found to fit the Avrami equation after the fraction transformed was normalized to unity at the completion of the reaction and a transformation-start time was determined. The transformation kinetics under isothermal conditions therefore were characterized in terms of the n and b parameters from the Avrami equation together with the transformation-start times. The parameter n was found to be independent of temperature over the range studied (645 to 760 {\textdaggerdbl}C) and to have values of 0.99 and 1.33 for the ferrite and pearlite reactions, respectively. This indicates that the nucleation condition is essentially constant and site saturation occurs early in the transformation process. The continuous-cooling experiments were conducted at cooling rates of 2 to 150 {\textdaggerdbl}C per second to determine the transformation-start times for the ferrite and pearlite reactions and the completion time for transformation to pearlite under {CCT} conditions. Both reactions were found to obey the Additivity Principle for continuous cooling provided that the incubation (pre-transformation) period was not included in the transformation time. The isothermal transformation data and {CCT} transformation-start times have been incorporated in a mathematical model to predict continuous-cooling transformation kinetics that agree closely with measurements made at three cooling rates.},
	number = {4},
	urldate = {2012-11-05},
	journal = {Metallurgical and Materials Transactions A},
	author = {Hawbolt, E. and Chau, B. and Brimacombe, J.},
	year = {1985},
	pages = {565--578},
	file = {Hawbolt et al. - 1985 - Kinetics of austenite-ferrite and austenite-pearli.pdf:/home/ken/.zotero/zotero/profile.default/zotero/storage/GIIHUV8W/Hawbolt et al. - 1985 - Kinetics of austenite-ferrite and austenite-pearli.pdf:application/pdf}
