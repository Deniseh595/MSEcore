	file = {APS Full Text PDF:/home/ken/.zotero/zotero/profile.default/zotero/storage/AD8I9X6K/Ilett et al. - 1995 - Phase behavior of a model colloid-polymer mixture.pdf:application/pdf;APS Snapshot:/home/ken/.zotero/zotero/profile.default/zotero/storage/EJ3H4TTH/p1344_1.html:text/html}
}

@article{hoare_physical_1975,
	title = {Physical cluster mechanics: Statistical thermodynamics and nucleation theory for monatomic systems},
	volume = {24},
	issn = {0001-8732},
	shorttitle = {Physical cluster mechanics},
	url = {http://www.tandfonline.com/doi/abs/10.1080/00018737500101481},
	doi = {10.1080/00018737500101481},
	abstract = {Abstract We extend previous computations of mechanical stability of atomic microclusters to the realm of statistical thermodynamics, obtaining thermodynamic functions for small, solid-like Van der Waals clusters of less than some 100 atoms possessing non-crystalline structures of {\textquoteleft}polytetrahedral{\textquoteright} type. These are shown to be almost invariably at a thermodynamic advantage over alternative lattice structures of the same number of atoms, at least for the Lennard-Jones potential in the harmonic-oscillator/rigid-rotor approximation. The dependence of thermodynamic functions upon cluster size appears to be essentially monotonic in the number of internal degrees of freedom; although there are certain exceptional structures, particularly with icosahedral symmetry, there proves to be little evidence for the occurrence of {\textquoteleft}magic numbers{\textquoteright} for stability at any temperature and within the size-range considered. Particular attention is given to the heat capacity of model systems in relation to their vibrational spectra. The Debye T 3 law appears reasonably well obeyed at low temperatures with no evidence for the existence of either {\textquoteleft}soft modes{\textquoteright} or distinct surface contributions. The results for the free energy of formation of minimal clusters $\Delta$G f are then applied to the computation of nucleation rates in terms of the Becker-D{\"o}ring-Volmer-Zeldovitch quasi-equilibrium theory. Gibbsian behaviour in the form of a maximum in the curve of $\Delta$G f versus size is observed with a critical nuclear size at realistic temperatures and pressures of the order of that predicted by macroscopic liquid-drop theories. These figures and those derived for nucleation rate and critical supersaturation appear remarkably insensitive to the details of the model used, in particular to the distinction between {\textquoteleft}microcrystalline{\textquoteright} and {\textquoteleft}amorphous{\textquoteright} atomistic models. The general status of atomistic nucleation theory is critically examined in the light of these and similar results.},
	number = {5},
	urldate = {2013-03-12},
	journal = {Advances in Physics},
	author = {Hoare, M.R. and Pal, P.},
	year = {1975},
	pages = {645--678},
	file = {Snapshot:/home/ken/.zotero/zotero/profile.default/zotero/storage/HQ95HRJ9/00018737500101481.html:text/html}
}

