@article{ilett_phase_1995,
	title = {Phase behavior of a model colloid-polymer mixture},
	volume = {51},
	url = {http://link.aps.org/doi/10.1103/PhysRevE.51.1344},
	doi = {10.1103/PhysRevE.51.1344},
	abstract = {We report a detailed experimental study of a model hard-sphere colloid plus nonadsorbing polymer mixture in which the depletion effect causes phase separation. The key parameter that determines the topology of the phase diagram is the ratio $\xi$=rg/a of the radius of gyration (rg) of the polymer molecules to the radius (a) of the colloidal particles. At small $\xi$, the addition of the polymer simply expands the colloidal fluid-crystal coexistence region of the pure hard-sphere system. For larger values of $\xi$, however, a three-phase coexistence of colloidal gas, liquid, and crystal phases is observed. The crossover between the two topologies is found at $\xi$\~{}0.25. These experimental observations are compared with the predictions of a recent statistical mechanical theory for such mixtures [Lekkerkerker et al., Europhys. Lett. 20, 559 (1992)]. The relevance of our results to the current debate about the conditions necessary for the existence of a {\textquoteleft}{\textquoteleft}liquid{\textquoteright}{\textquoteright} state is discussed.},
	number = {2},
	urldate = {2013-03-14},
	journal = {Physical Review E},
	author = {Ilett, S. M. and Orrock, A. and Poon, W. C. K. and Pusey, P. N.},
	month = feb,
