@article{raj_apparent_1989,
	title = {Apparent activation volume for creep of copper and alpha brass at intermediate temperatures},
	volume = {24},
	issn = {0022-2461, 1573-4803},
	url = {http://link.springer.com/article/10.1007/BF01139041},
	doi = {10.1007/BF01139041},
	abstract = {Experimental measurements of the apparent activation volume for creep,V *, of Cu and Cu-30\% Zn conducted at intermediate temperatures showed two types of strain dependencies. At the lower temperatures and higher stresses,V * decreased with increasing creep strain, $\varepsilon$, while at higher temperatures and lower stresses,V * was essentially independent of strain. The low temperature-high stress behaviour for Cu and Cu-30\% Zn was found to be consistent with the dominance of a dislocation intersection mechanism. The high temperature-low stress data for the pure metals suggest that the rate-controlling process involves the nonconservative motion of jogs on screw dislocations. For the latter conditions, an additional contribution from solute drag-limited dislocation glide also appears to be important in governing the creep behaviour of the alloy.},
